<% layout('/layouts/boilerPlate') -%>


  <div class="row row-cols-lg-3 row-cols-md-2 row-cols-1">
    <% for(let list of listingData){ %> <% if (list && list.title) { %>
    <a href="/listings/<%= list._id %>" class="listing-link">
      <div class="card col">
        <div class="card-img-overlay"></div>
        <img
          src="<%= list.image %>"
          class="card-img-top"
          alt="..."
          style="height: 20rem"
          ;
        />
        <div class="card-body">
          <b> <h5 style="color: black"><%= list.title %></h5></b>
          <p><%= list.price.toLocaleString("en-IN") %>/Night</p>
        </div>
      </div>
    </a>
    <% } %> <% } %>
  </div>
</body>
